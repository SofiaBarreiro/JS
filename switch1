function Mostrar()
{
//tomo la edad  
var mesDelAño = document.getElementById('mes').value;

switch(mesDelAño) {
	case "Enero":
	alert("que comiences bien las clases");
	break;
	case "Marzo":
	alert("a clases");
	break;
	case "Julio":
	alert("se vienen las vacaciones");
	break;
	case "Diciembre":
	alert("Felices Fiestas");
	break
	default:
	break
}
console.log


}//FIN DE LA FUNCIÓN
